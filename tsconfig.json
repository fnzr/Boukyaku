{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./build",
    "rootDir": "./",
    "strict": true,
    "baseUrl": "./src/",
    "paths": {
      "@*": [
        "./*"
      ]
    },
    "esModuleInterop": true
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "node_modules"
  ]
}